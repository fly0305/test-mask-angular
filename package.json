{
  "name": "@msafi/text-mask",
  "version": "0.0.0",
  "description": "Conforms string to given mask or pattern, and can be hooked up to input element",
  "main": "index.js",
  "scripts": {
    "website": "node ./website/server.js",
    "website:build": "./node_modules/.bin/webpack -p --config website/webpack.buildWebsite.js",
    "test": "npm run lint && npm run core:test && npm run react:test && npm run vanilla:test",
    "core:build": "./node_modules/.bin/webpack -p --config core/webpack.buildCore.js",
    "core:tdd": "./node_modules/.bin/mocha --compilers js:babel-core/register core/test --recursive --watch",
    "core:test": "./node_modules/.bin/mocha --compilers js:babel-core/register core/test --recursive",
    "core:publish": "sh core/publish.sh",
    "core:verify": "./node_modules/.bin/mocha --compilers js:babel-core/register core/test --recursive --verify",
    "react:watch": "./node_modules/.bin/webpack --watch --config integrations/react/webpack.buildReactIntegration.js",
    "react:tdd": "./node_modules/.bin/mocha --compilers jsx:babel-core/register integrations/react/test --require ./common/domSetup.js --recursive --watch",
    "react:test": "./node_modules/.bin/mocha --compilers jsx:babel-core/register integrations/react/test --require ./common/domSetup.js --recursive",
    "react:build": "./node_modules/.bin/webpack -p --config integrations/react/webpack.buildReactIntegration.js",
    "react:publish": "sh integrations/react/publish.sh",
    "react:verify": "./node_modules/.bin/mocha --compilers jsx:babel-core/register integrations/react/test --require ./common/domSetup.js --recursive --verify",
    "vanilla:watch": "./node_modules/.bin/webpack --watch --config integrations/vanilla/webpack.buildVanillaIntegration.js",
    "vanilla:tdd": "./node_modules/.bin/mocha --compilers js:babel-core/register integrations/vanilla/test --require ./common/domSetup.js --recursive --watch",
    "vanilla:test": "./node_modules/.bin/mocha --compilers js:babel-core/register integrations/vanilla/test --require ./common/domSetup.js --recursive",
    "vanilla:build": "./node_modules/.bin/webpack -p --config integrations/vanilla/webpack.buildVanillaIntegration.js",
    "vanilla:publish": "sh integrations/vanilla/publish.sh",
    "vanilla:verify": "./node_modules/.bin/mocha --compilers js:babel-core/register integrations/vanilla/test --require ./common/domSetup.js --recursive --verify",
    "git:done": "git checkout gh-pages && git merge master && git checkout master && git push origin master && git push origin gh-pages",
    "ci": "npm run test && npm run core:verify && npm run react:verify && npm run vanilla:verify",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msafi/text-mask.git"
  },
  "author": "M.K. Safi <msafi@msafi.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/msafi/text-mask/issues"
  },
  "homepage": "https://github.com/msafi/text-mask#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.4.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "jsdom": "^8.0.2",
    "lodash": "^4.0.0",
    "mocha": "^2.3.4",
    "mocha-dynamic-tests": "^1.1.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.0.0",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "stats-webpack-plugin": "^0.3.0",
    "style-loader": "^0.13.0",
    "testdom": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
